{
  "name": "sheetstorm-lit",
  "version": "1.0.0",
  "description": "A modal component for data import via a guided wizard.",
  "sideEffects": false,
  "main": "src/index.ts",
  "scripts": {
    "start": "concurrently \"npm run lit:dev\" \"npm run storybook:dev\" \"npm run test:watch\" --names \"component,storybook,tests\"",
    "build": "npm run lit:prod && npm run storybook:build",
    "lit:dev": "npx rollup -cw",
    "lit:prod": "npx rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "npm run storybook:build && npm run storybook:serve",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,html}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@vaadin/grid": "^22.1.0",
    "date-fns": "^2.23.0",
    "lit": "^3.1.4",
    "papaparse": "^5.4.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^2.0.2",
    "@eslint/js": "^9.10.0",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/builder-vite": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/web-components": "^8.3.1",
    "@storybook/web-components-vite": "^8.3.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.19.2",
    "globals": "^15.9.0",
    "jest": "29.7.0",
    "rollup": "^4.21.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.3.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "ts-transform-paths": "^3.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.5.0",
    "typescript-transform-paths": "^3.5.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "lit",
    "modal",
    "wizard",
    "data-import",
    "flatfile",
    "flatfile alternative"
  ],
  "author": "Luke Zehrung",
  "license": "MIT"
}
